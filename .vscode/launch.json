            "projectName": "user-service",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Spring Boot-EmailServiceApplication<email-service>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.danioclana.email_service.EmailServiceApplication",
            "projectName": "email-service",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
